generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model AllJamaah{
  Id Int @id @default(autoincrement())
  Nama String
  Ismale Boolean?
  Age Int
  Province String
  Group String
  Lat Float? 
  Lng Float?
  Temp Float?
  Humid Float?
}

model UserCommunication{
  Id Int @id @default(autoincrement())
  UserId String @unique
  Teleid String?
}